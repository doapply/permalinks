{
  "name": "permalinks",
  "description": "Easily add powerful permalink or URL routing/URL rewriting capablities to any node.js project. Can be used in static site generators, build systems, web applications or anywhere you need to do path or URL transformation.",
  "version": "1.2.1",
  "homepage": "https://github.com/permalinks/permalinks",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "permalinks/permalinks",
  "bugs": {
    "url": "https://github.com/permalinks/permalinks/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "deep-bind": "^0.3.0",
    "diacritics-map": "^0.1.0",
    "extend-shallow": "^3.0.2",
    "get-value": "^2.0.6",
    "handlebars": "^4.0.11",
    "kind-of": "^6.0.2",
    "mixin-deep": "^1.3.0",
    "parse-filepath": "^1.0.2",
    "snapdragon-lexer": "^2.0.0",
    "use": "^3.1.0",
    "vinyl": "^2.1.0"
  },
  "devDependencies": {
    "mocha": "^3.5.3",
    "moment": "^2.20.1",
    "randomatic": "^3.0.0",
    "gulp-format-md": "^1.0.0"
  },
  "keywords": [
    "blog entry",
    "blog post",
    "filepath",
    "link",
    "path",
    "permalinks",
    "permanent link",
    "rewrite",
    "rewriting",
    "route",
    "routes",
    "routing",
    "transformation",
    "uri",
    "url",
    "URL rewriting",
    "url-routing"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "handlebars",
        "parse-filepath",
        "vinyl"
      ]
    },
    "reflinks": [
      "file",
      "handlebars",
      "handlebars-helpers",
      "helpers",
      "locals",
      "parse-filepath",
      "structure",
      "template-helpers",
      "vinyl"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
